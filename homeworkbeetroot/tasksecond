stock = {
    "banana": 6,
    "apple": 0,
    "orange": 32,
    "pear": 15
}
prices = {
    "banana": 4,
    "apple": 2,
    "orange": 1.5,
    "pear": 3
}
def fruits(stock, prices):
    s = 0
    for key in stock:
        s += stock[key] * prices[key]
    return s

s = fruits(stock, prices)
print(s)
